														<div class="col-sm-8">
															{{ form_widget(registrationForm.societe) }}
														</div>
													</div>
												</div>
												<div class="col-lg-6">
													<div class="form-group row">
														<div class="col-sm-4 col-form-label">
															<label for="titre">{{ form_label(registrationForm.adresse) }}</label>
														</div>
														<div class="col-sm-8">
