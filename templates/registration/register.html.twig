														</div>
														<div class="col-sm-8">
															{{ form_widget(registrationForm.pays) }}
														</div>
													</div>
												</div>
												<div class="col-lg-6">
													<div class="form-group row">
														<div class="col-sm-4 col-form-label">
															<label for="titre">{{ form_label(registrationForm.siteweb) }}</label>
														</div>
														<div class="col-sm-8">
															{{ form_widget(registrationForm.siteweb) }}
														</div>
													</div>
												</div>
											</div>
											<div class="row">
												<div class="col-lg-6">
													<div class="form-group row">
														<div class="col-sm-4 col-form-label">
															<label for="titre">{{ form_label(registrationForm.password.first) }}</label>
														</div>
														<div class="col-sm-8">
															{{ form_widget(registrationForm.password.first,{'class': 'form-control'}) }}
														</div>
													</div>
												</div>
												<div class="col-lg-6">
													<div class="form-group row">
														<div class="col-sm-4 col-form-label">
															<label for="titre">{{ form_label(registrationForm.password.second) }}</label>
														</div>
														<div class="col-sm-8">
															{{ form_widget(registrationForm.password.second) }}
														</div>
													</div>
												</div>
											</div>
											<div class="row">
												<div class="col-lg-6">
													<div class="form-group row">
														<div class="col-sm-4 col-form-label">
															<label for="titre">{{ form_label(registrationForm.roles) }}</label>
														</div>
														<div class="col-sm-8">
															{{ form_widget(registrationForm.roles) }}
														</div>
													</div>
												</div>
											</div>
											<div class="row">
												<div class="col-lg-6">
