<html lang="en">

	<head>

		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<meta name="description" content="">
		<meta name="author" content="">

		<title>B2B</title>

		<!-- Custom fonts for this template-->
		<link href="../bundles/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
		<link
		href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

		<!-- Custom styles for this template-->
		<link href="../bundles/css/sb-admin-2.min.css" rel="stylesheet">

	</head>


	<body class="bg-gradient">
		<div
			class="container">

			<!-- Outer Row -->
			<div class="row justify-content-center">

				<div class="col-xl-8 col-lg-12 col-md-9">

					<div class="card o-hidden border-0 shadow-lg my-5">
						<div
							class="card-body p-0">
							<!-- Nested Row within Card Body -->
							<div class="row">


								<div class="login100-pic js-tilt" data-tilt>

									<img src="../bundles/assets/img/logo/login-lanczos33.jpg" alt="IMG">
								</div>
								<div class="col-lg-6">
									<div class="p-5">
										<div class="text-center">
											<h1 class="h3 mb-4 font-weight-normal">Veuillez vous connecter !
											</h1>
											<form method="post">
												{% if error %}
													<div class="alert alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
												{% endif %}

												{% if app.user %}
													<div class="mb-3">
														Vous êtes connecté en tant que
														{{ app.user.username }},
														<a href="{{ path('app_logout') }}">se déconnecter</a>
													</div>
												{% endif %}

												<div>
													<div class="form-group">
														<div class="input-group-prepend">
															<span class="input-group-text">
																<i class="fas fa-user"></i>
															</span>

															<input type="email" value="{{ last_username }}" name="email" placeholder="Enter Email Address..." id="inputEmail" class="form-control" required autofocus>

														</div>
													</div>


													<div class="form-group">

														<div class="input-group-prepend">
															<span class="input-group-text">
																<i class="fas fa-key"></i>
															</span>

															<input type="password" name="password" placeholder="password" id="inputPassword" class="form-control" required>
														</div>
													</div>
													<input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">


													<div class="checkbox mb-3" class="fa fa-align-left">
														<label>
															<input type="checkbox" name="_remember_me">
															Remember me
														</label>


														<div class="text-center">
															<button class="btn btn-primary btn-user btn-block" type="submit">
																Login
															</button>


														</form>
														<div class="text-center">
															<p>
																<a href="reset-password">Mot de passe oublié</a>
															</p>
														</div>


														<div class="text-center">
															<a href="register">Créer un compte!</a>
														</div>
													</div>
												</div>
											</div>
										</div>
									</div>
								</div>

							</div>

						</div>

					</div>
				</body>
				<!-- Bootstrap core JavaScript-->
				<script src="../bundles/vendor/jquery/jquery.min.js"></script>
				<script src="../bundles/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

				<!-- Core plugin JavaScript-->
				<script src="../bundles/vendor/jquery-easing/jquery.easing.min.js"></script>

				<!-- Custom scripts for all pages-->
				<script src="../bundles/js/sb-admin-2.min.js"></script>
